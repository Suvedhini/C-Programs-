#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cout<<"Enter the value of a:"<<"\n";
    cin>>a;
    cout<<"Enter the value of b:"<<"\n";
    cin>>b;
    cout<<"Enter the value of c:"<<"\n";
    cin>>c;
    if(a>b)
    {
        if(a>c)
        {
            cout<<a<<"is greater!";
        }
        else
        {
            cout<<c<<"is greater!";
        }
    }
    else
    {
        if(b>c)
        {
            cout<<b<<"is greater!";
        }
        else
        {
            cout<<c<<"is greater!";
        }
    }
    return 0;
}
